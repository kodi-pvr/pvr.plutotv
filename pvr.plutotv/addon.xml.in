<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="pvr.plutotv"
  version="22.2.0"
  name="Pluto.tv PVR Client"
  provider-name="Team Kodi, flubshi">
  <requires>@ADDON_DEPENDS@
    <import addon="inputstream.adaptive" minversion="22.0.0"/>
